{
  "name": "@anthonynichols/import-sort-style-belvedere",
  "version": "1.1.3",
  "author": "Anthony Nichols <hi@anthonynichols.me> (https://github.com/anthonynichols)",
  "description": "Import sort configuration for Belvedere apps",
  "keywords": [
    "import",
    "sort",
    "style",
    "import-sort",
    "import-sort-style"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonynichols/import-sort-style-belvedere.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.ts\"",
    "watch": "tsc -w -p tsconfig.build.json",
    "watch:test": "mocha -w --require ts-node/register --recursive \"test/**/*.ts\""
  },
  "dependencies": {
    "import-sort-parser": "^6.0.0",
    "import-sort-style": "^6.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "chai": "^4.2.0",
    "import-sort": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "mocha": "^7.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
